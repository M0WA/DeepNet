function execute_mysql()
{
  source "{DB_CONFIG_DIR}/db.generator.mysql.conf"
  EXEC_FILENAME=$1
  echo "executing ${EXEC_FILENAME}"
  echo "source ${EXEC_FILENAME};" | mysql -h${DBHOST} -P${DBPORT} -p${DBPASS} -u${DBUSER}
}
